			<key>comment</key>
			<string>Matches empty tags.</string>
			<key>match</key>
			<string>(?x)
				(^\s*)?							# 1 - Leading whitespace
					(							# 2 - meta.embedded.line.empty.php
						(						# 3 - Open Tag
							(?:
								((?&lt;=\?&gt;)&lt;)		# 4 - Consecutive tags
